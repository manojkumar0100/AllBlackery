# Frontend Environment Configuration for AllBlackery

# Backend API Configuration
VITE_API_BASE_URL=http://localhost:8000/api/v1/
VITE_BACKEND_URL=http://localhost:8000

# Google OAuth Configuration
# Replace with your actual Google OAuth credentials
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Stripe Configuration
# Replace with your actual Stripe publishable key
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# App Configuration
VITE_APP_NAME=AllBlackery
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Premium Black Fashion E-commerce Platform

# CDN Configuration (for images and assets)
VITE_CDN_URL=https://cdn.allblackery.com

# Analytics Configuration
VITE_GOOGLE_ANALYTICS_ID=your-google-analytics-id
VITE_FACEBOOK_PIXEL_ID=your-facebook-pixel-id

# Map Configuration (for store locator)
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Social Media Configuration
VITE_FACEBOOK_APP_ID=your-facebook-app-id
VITE_INSTAGRAM_HANDLE=@allblackery
VITE_TWITTER_HANDLE=@allblackery

# Development Configuration
VITE_DEV_MODE=true
VITE_API_TIMEOUT=30000
VITE_ENABLE_LOGS=true

# Feature Flags
VITE_ENABLE_GOOGLE_AUTH=true
VITE_ENABLE_STRIPE_PAYMENTS=true
VITE_ENABLE_WISHLIST=true
VITE_ENABLE_CART=true
VITE_ENABLE_REVIEWS=true
VITE_ENABLE_RECOMMENDATIONS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_LIVE_CHAT=true
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_PWA=true